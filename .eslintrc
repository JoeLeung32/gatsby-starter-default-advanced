{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "linebreak-style": [
      0,
      "windows"
    ],
    "react/no-danger": "off",
    "react/no-danger-with-children": "error",
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "document": true,
    "window": true,
    "navigator": true
  }
}
